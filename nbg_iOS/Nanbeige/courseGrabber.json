{
	"grouped": true,
	"title": "导入课程",
    "controllerName": "CPCourseGrabberViewController",
	"sections": [
				 {
				 "elements": [
							  {
							  "type":"CPEntryElement",
							  "placeholder":"用户名",
							  "bind":"textValue:username",
							  "autocorrectionType":"No",
							  "autocapitalizationType":"None"
							  },
							  {
							  "type":"CPEntryElement",
							  "placeholder":"密码",
							  "secureTextEntry":true,
							  "bind":"textValue:password"
							  }
							  ]
				 },
				 {
				 "bind":"iterate:captcha",
				 "elementTemplate": {
				 "type":"CPEntryElement",
				 "bind":"textValue:captcha, placeholder:placeholder",
				 "autocorrectionType":"No",
				 "autocapitalizationType":"None"
				 }
				 }
				 ]
}