{
	"grouped": true,
	"title": "用 Email 地址注册",
    "controllerName": "CPSignupEmailViewController",
	"sections": [
				 {
				 "elements": [
							  {
							  "type":"CPEntryElement",
							  "placeholder":"Email",
							  "keyboardType":"EmailAddress",
							  "bind":"textValue:email",
							  "autocorrectionType":"No",
							  "autocapitalizationType":"None"
							  },
							  {
							  "type":"CPEntryElement",
							  "placeholder":"密码",
							  "secureTextEntry":true,
							  "bind":"textValue:password"
							  }
							  ]
				 },
				 {
				 "elements":[
							 {
							 "type":"CPEntryElement",
							 "placeholder":"昵称",
							 "bind":"textValue:nickname"
							 }
							 ]
				 }
				 ]
}